{
  "name": "blpro",
  "version": "1.0.0",
  "description": "My polished Nuxt.js project",
  "author": "jiangconghu",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development HOST=0.0.0.0 PORT=3333 nodemon server/index.js --watch server --exec babel-node",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js --exec babel-node",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "browserslist": [
    "> 1%",
    "last 15 versions",
    "last 15 Chrome versions",
    "last 15 Firefox versions",
    "Safari >= 6",
    "ie > 8"
  ],
  "dependencies": {
    "cross-env": "^5.2.0",
    "nuxt": "^2.0.0",
    "koa": "^2.5.2",
    "element-ui": "^2.4.6",
    "@nuxtjs/axios": "^5.0.0"
  },
  "devDependencies": {
    "acorn": "^6.0.2",
    "autoprefixer": "^9.3.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^3.18.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.9.4",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "sass-loader": "^7.1.0"
  }
}
